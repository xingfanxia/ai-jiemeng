# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# AI Provider API Keys
ANTHROPIC_API_KEY=your-anthropic-api-key
GOOGLE_AI_API_KEY=your-google-ai-api-key

# AI Configuration
# NOTE: Model selection is now controlled via PostHog feature flags
# The feature flag 'jiemeng-ai-model-experiment' controls which model is used
# Legacy A/B testing config (deprecated, kept for backwards compatibility):
AI_DEFAULT_PROVIDER=gemini
AI_AB_TEST_ENABLED=false

# Optional: Override default models (fallback if feature flag not available)
# CLAUDE_MODEL=claude-opus-4-5-20251101
# GEMINI_MODEL=gemini-3-pro-preview

# PostHog Analytics
NEXT_PUBLIC_POSTHOG_KEY=your-posthog-public-key
NEXT_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com

# PostHog API (for creating experiments programmatically)
# POSTHOG_PERSONAL_API_KEY=your-posthog-personal-api-key
# POSTHOG_PROJECT_ID=your-project-id

# App Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
